{
    "name": "ecommit/deploy-rsync-bundle",
    "description": "Deploy Symfony project with Rsync",
    "keywords": ["symfony", "deploy", "rsync", "ssh", "symfony-bundle"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Ecommit\\DeployRsyncBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ecommit\\DeployRsyncBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "symfony/config": "^6.4|^7.3|^8.0",
        "symfony/console": "^6.4|^7.3|^8.0",
        "symfony/dependency-injection": "^6.4|^7.3|^8.0",
        "symfony/framework-bundle": "^6.4|^7.3|^8.0",
        "symfony/http-kernel": "^6.4|^7.3|^8.0",
        "symfony/process": "^6.4|^7.3|^8.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.88",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^10"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
