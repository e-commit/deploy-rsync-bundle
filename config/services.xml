<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Ecommit\DeployRsyncBundle\Command\DeployRsyncCommand" class="Ecommit\DeployRsyncBundle\Command\DeployRsyncCommand" public="false">
            <argument>%ecommit_deploy_rsync.environments%</argument>
            <argument>%ecommit_deploy_rsync.rsync%</argument>
            <argument>%kernel.project_dir%</argument>
            <tag name="console.command" />
        </service>
    </services>
</container>
